{
    "name": "@pinnacle0/web-ui",
    "version": "0.1.69",
    "author": "Pinnacle",
    "license": "MIT",
    "sideEffects": [
        "*.less",
        "*.css"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pinnacle0/pinnacle-frontend-web-ui"
    },
    "scripts": {
        "test": "jest --config config/jest.config.js",
        "test-e2e": "testcafe \"chrome --headless --window-size='800,600' --app-shell-host-window-size='800,600' --content-shell-host-window-size='800,600'\" \"./e2e/**/*.e2e.{ts,tsx}\" --skip-js-errors --screenshots path=artifacts/screenshots --ts-config-path ./e2e/tsconfig.json",
        "ts-node": "TS_NODE_PROJECT=./script/tsconfig.json ../../node_modules/.bin/ts-node",
        "build": "yarn ts-node script/build.ts",
        "start": "yarn ts-node script/start.ts",
        "lint": "yarn ts-node script/lint.ts",
        "format": "yarn ts-node script/format.ts"
    },
    "dependencies": {
        "@ant-design/icons": "4.2.2",
        "antd": "4.6.4",
        "braft-editor": "2.3.9",
        "braft-extensions": "0.1.0",
        "moment": "2.29.1"
    },
    "peerDependencies": {
        "react": ">=16.13.0",
        "react-dom": ">=16.13.0",
        "react-router-dom": ">=5.2.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.11.4",
        "@testing-library/react": "11.0.4",
        "@testing-library/user-event": "12.1.5",
        "@types/draft-js": "0.10.44",
        "@types/html-webpack-plugin": "3.2.3",
        "@types/react": "16.9.52",
        "@types/react-dom": "16.9.8",
        "@types/react-router-dom": "5.1.6",
        "@types/webpack": "4.41.22",
        "@types/webpack-dev-server": "3.11.0",
        "canvas": "2.6.1",
        "css-loader": "4.3.0",
        "file-loader": "6.1.0",
        "html-webpack-plugin": "4.4.1",
        "image-type": "4.1.0",
        "less": "3.12.2",
        "less-loader": "7.0.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "5.2.0",
        "ssim.js": "3.3.0",
        "style-loader": "1.2.1",
        "stylelint": "13.7.2",
        "stylelint-webpack-plugin": "2.1.0",
        "testcafe": "1.9.1",
        "ts-loader": "8.0.3",
        "webpack": "4.44.1",
        "webpack-dev-server": "3.11.0"
    },
    "engines": {
        "node": ">= 12.16"
    }
}
