{
    "name": "@pinnacle0/web-ui",
    "version": "0.1.67",
    "author": "Pinnacle",
    "license": "MIT",
    "sideEffects": [
        "*.less",
        "*.css"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pinnacle0/pinnacle-frontend-web-ui"
    },
    "scripts": {
        "test": "jest --config config/jest.config.js",
        "test-e2e": "testcafe \"chrome --headless --window-size='800,600' --app-shell-host-window-size='800,600' --content-shell-host-window-size='800,600'\" \"./e2e/**/*.e2e.{ts,tsx}\" --skip-js-errors --screenshots path=artifacts/screenshots --ts-config-path ./e2e/tsconfig.json",
        "ts-node": "TS_NODE_PROJECT=./script/tsconfig.json ./node_modules/.bin/ts-node",
        "build": "yarn ts-node script/build.ts",
        "start": "yarn ts-node script/start.ts",
        "lint": "eslint --config config/.eslintrc.js --ext .js,.jsx,.ts,.tsx .",
        "format": "prettier --config config/prettier.config.js --write \"**/*.{js,json,jsx,less,ts,tsx}\""
    },
    "dependencies": {
        "@ant-design/icons": "4.2.2",
        "antd": "4.6.4",
        "braft-editor": "2.3.9",
        "braft-extensions": "0.1.0",
        "moment": "2.28.0"
    },
    "peerDependencies": {
        "react": ">= 16.13.0",
        "react-dom": ">= 16.13.0",
        "react-router-dom": ">= 5.2.0"
    },
    "devDependencies": {
        "@pinnacle0/eslint-plugin": "0.0.6",
        "@testing-library/jest-dom": "5.11.4",
        "@testing-library/react": "11.0.4",
        "@testing-library/user-event": "12.1.5",
        "@types/draft-js": "0.10.44",
        "@types/eslint": "7.2.2",
        "@types/fs-extra": "9.0.1",
        "@types/html-webpack-plugin": "3.2.3",
        "@types/jest": "26.0.13",
        "@types/node": "14.10.2",
        "@types/react": "16.9.49",
        "@types/react-dom": "16.9.8",
        "@types/react-router-dom": "5.1.5",
        "@types/webpack": "4.41.22",
        "@types/webpack-dev-server": "3.11.0",
        "@typescript-eslint/eslint-plugin": "4.1.1",
        "@typescript-eslint/parser": "4.1.1",
        "canvas": "2.6.1",
        "css-loader": "4.3.0",
        "eslint": "7.9.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-jest": "24.0.1",
        "eslint-plugin-jest-dom": "3.2.3",
        "eslint-plugin-react": "7.20.6",
        "eslint-plugin-react-hooks": "4.1.2",
        "eslint-plugin-testing-library": "3.8.0",
        "file-loader": "6.1.0",
        "fs-extra": "9.0.1",
        "html-webpack-plugin": "4.4.1",
        "image-type": "4.1.0",
        "jest": "26.4.2",
        "less": "3.12.2",
        "less-loader": "7.0.1",
        "prettier": "2.1.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "5.2.0",
        "ssim.js": "3.3.0",
        "style-loader": "1.2.1",
        "stylelint": "13.7.1",
        "stylelint-config-prettier": "8.0.2",
        "stylelint-config-standard": "20.0.0",
        "stylelint-webpack-plugin": "2.1.0",
        "testcafe": "1.9.1",
        "ts-jest": "26.3.0",
        "ts-loader": "8.0.3",
        "ts-node": "9.0.0",
        "typescript": "4.0.2",
        "webpack": "4.44.1",
        "webpack-dev-server": "3.11.0",
        "yargs": "16.0.3"
    },
    "engines": {
        "node": ">= 12.16"
    }
}
