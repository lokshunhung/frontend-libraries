{
    "name": "@pinnacle0/eslint-plugin",
    "version": "0.1.23",
    "license": "MIT",
    "scripts": {
        "run-script": "ts-node --project config/tsconfig.script.json",
        "build": "yarn run-script script/build.ts",
        "codegen": "yarn run-script script/codegen.ts",
        "format": "yarn run-script script/format.ts",
        "new-rule": "yarn run-script script/new-rule.ts",
        "test": "jest --config config/jest.config.js"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "5.3.1",
        "@typescript-eslint/experimental-utils": "5.3.1",
