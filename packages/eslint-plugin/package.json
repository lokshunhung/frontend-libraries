        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.5.0"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "22.0.0",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.3.0",
        "rollup": "2.73.0",
        "rollup-plugin-typescript2": "0.31.2"
