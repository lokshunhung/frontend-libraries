        "eslint-plugin-react": "7.27.0",
        "eslint-plugin-react-hooks": "4.3.0"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "21.0.1",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.0.6",
        "rollup": "2.59.0",
        "rollup-plugin-typescript2": "0.30.0"
