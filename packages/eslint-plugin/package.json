{
    "name": "@pinnacle0/eslint-plugin",
    "version": "0.0.7",
    "license": "MIT",
    "main": "dist/index.js",
    "targets": {
        "main": {
            "minify": false,
            "outputFormat": "commonjs",
            "sourceMap": false
        }
    },
    "scripts": {
        "run-script": "ts-node --project config/tsconfig.script.json",
        "build": "yarn run-script script/build.ts",
        "codegen": "yarn run-script script/codegen.ts",
        "format": "yarn run-script script/format.ts",
        "lint": "yarn run-script script/lint.ts",
        "new-rule": "yarn run-script script/new-rule.ts",
        "test": "jest --config config/jest.config.js"
    },
    "dependencies": {
        "@typescript-eslint/experimental-utils": "^4.7.0"
    },
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "@typescript-eslint/parser": "^4.7.0",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0"
    },
    "devDependencies": {
        "parcel": "2.0.0-beta.1"
    },
    "optionalDependencies": {
        "eslint-plugin-jest": "^24.1.1",
        "eslint-plugin-jest-dom": "^3.2.4",
        "eslint-plugin-testing-library": "^3.10.0"
    },
    "files": [
        "./dist/*",
        "./README.md",
        "./LICENSE.md"
    ],
    "engines": {
        "node": ">=12.19.x"
    }
}
