{
    "name": "@pinnacle0/awesome-readme-image",
    "description": "This is a useless project to test out new react jsx transform with babel and parcel.",
    "version": "0.0.1",
    "private": true,
    "main": "dist/build.js",
    "targets": {
        "main": {
            "context": "node",
            "includeNodeModules": false
        }
    },
    "scripts": {
        "start": "parcel serve src/index.html",
        "build": "parcel build src/build.tsx --no-source-maps && node dist/build.js"
    },
    "dependencies": {
        "core-js": "3.22.3",
        "react": "18.1.0",
        "react-dom": "18.1.0"
    },
    "devDependencies": {
        "@babel/core": "7.17.10",
        "@babel/preset-env": "7.17.10",
        "@babel/preset-react": "7.16.7",
        "@babel/preset-typescript": "7.16.7",
        "@parcel/optimizer-data-url": "2.5.0",
        "@parcel/transformer-inline-string": "2.5.0",
        "@parcel/transformer-less": "2.5.0",
        "@parcel/transformer-typescript-tsc": "2.5.0",
        "parcel": "2.5.0"
    },
    "engines": {
        "node": ">= 16.0.0"
    }
}
